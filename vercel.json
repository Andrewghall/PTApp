{
  "buildCommand": "export $(cat .env.production | grep -v '^#' | xargs) && npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/_expo/(.*)",
      "dest": "/_expo/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
